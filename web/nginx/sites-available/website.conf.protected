server {
    listen 80 default_server;
    return 301 https://$server_addr$request_uri;
}

server{
	listen 443 ssl;
	ssl_certificate /etc/nginx/ssl/webnews.crt;
	ssl_certificate_key /etc/nginx/ssl/webnews.key;

	location / {
        root /local/files;
        index start.html;
    }
   location /files/ {
	root /local;
	autoindex on;
    }
}
